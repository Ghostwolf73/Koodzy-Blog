<%- include("../partials/header") -%>

    <h1>Compose</h1>
    <form action="/compose" method="post">

        <div class="form-group">
            <label for="postTitle">Title</label>
            <input type="text" autocomplete="off" name="postTitle" class="form-control">
        </div>

        <div class="form-group">
            <label for="postContent">Post</label>
            <textarea name="postContent" rows="6" cols="40" class="form-control"></textarea>
        </div>

        <button type="submit" name="button" value="publishComp" class="btn btn-primary">Publish</button>
    </form>




    <%- include("../partials/footer") -%>